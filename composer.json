{
    "name": "escherchia/laravel-scenario-logger",
    "description": "This package helps you to track your requests in a single snapshot",
    "type": "libr",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Escherchia\\LaravelScenarioLogger\\": "/src"
        },
        "files": [
            "src/helpers/helpers.php"
        ]
    },
    "require": {
        "laravel/framework" : "*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Escherchia\\LaravelScenarioLogger\\LSLServiceProvider"
            ]
        }
    }
}
